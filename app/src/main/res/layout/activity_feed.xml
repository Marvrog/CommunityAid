<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/backgroundColor"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="77dp"
        android:background="@drawable/appbar_design"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?attr/actionModeCloseDrawable" />



    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginLeft="108dp"
        android:layout_marginTop="28dp"
        android:fontFamily="sans-serif-thin"
        android:text="Feed"
        android:textAllCaps="true"
        android:textFontWeight="900"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="293dp"
        android:layout_height="55dp"
        android:background="@drawable/custom_edit_text"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="What's going on in your community?"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="101dp" />

    <ImageButton
        android:id="@+id/button5"
        android:layout_width="71dp"
        android:layout_height="54dp"
        android:background="@drawable/custom_edit_text"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="101dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="378dp"
        android:layout_height="415dp"
        android:background="@drawable/custom_edit_text"
        app:cardElevation="0dp"
        app:cardPreventCornerOverlap="false"
        app:cardCornerRadius="12dp"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="169dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">



            <androidx.cardview.widget.CardView
                android:layout_width="44dp"
                android:layout_height="46dp"
                android:elevation="0dp"
                app:cardElevation="0dp"
                android:padding="0px"
                android:outlineSpotShadowColor="@color/white"
                app:cardCornerRadius="10dp"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp" >

                <ImageView
                    android:id="@+id/editAccountImage"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    android:padding="0px"
                    app:srcCompat="@drawable/user" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bree"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="71dp"
                tools:layout_editor_absoluteY="18dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="3 weeks ago"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="38dp" />

            <androidx.cardview.widget.CardView
                android:layout_width="348dp"
                android:layout_height="152dp"
                android:elevation="0dp"
                app:cardElevation="0dp"
                android:padding="0px"
                android:outlineSpotShadowColor="@color/white"
                app:cardCornerRadius="10dp"
                tools:layout_editor_absoluteX="14dp"
                tools:layout_editor_absoluteY="72dp" >

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/user" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Massive pothole on lapaz road"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="245dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="340dp"
                android:layout_height="50dp"
                android:ellipsize="end"
                android:lines="3"
                android:text="@string/demo"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="275dp" />

            <Button
                android:id="@+id/button6"
                android:layout_width="94dp"
                android:layout_height="36dp"
                android:background="@drawable/button_shape"
                android:text="More"
                android:textSize="11sp"
                android:outlineAmbientShadowColor="@color/white"
                tools:layout_editor_absoluteX="262dp"
                tools:layout_editor_absoluteY="357dp" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="38dp"
                android:layout_height="39dp"
                app:srcCompat="@android:drawable/stat_notify_chat"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="357dp" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="38dp"
                android:layout_height="39dp"
                app:srcCompat="@android:drawable/stat_sys_headset"
                tools:layout_editor_absoluteX="108dp"
                tools:layout_editor_absoluteY="357dp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:text="21"
                tools:layout_editor_absoluteX="54dp"
                tools:layout_editor_absoluteY="361dp" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:text="213"
                tools:layout_editor_absoluteX="146dp"
                tools:layout_editor_absoluteY="361dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>